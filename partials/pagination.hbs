<div class="grid-12 text-center">
    <nav class="pagination">
        {{#if prev}}     
            <a class="newer-posts" href="{{page_url prev}}">{{t "Newer"}}</a>
        {{/if}}           
            <span class="page-number">{{t "Page"}} {{page}} {{t "of"}} {{pages}}</span>
        {{#if next}}              
            <a class="older-posts" href="{{page_url next}}">{{t "Older"}}</a>
        {{/if}}
    </nav>
</div>
